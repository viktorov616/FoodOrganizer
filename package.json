  "scripts": {
    "wds": "webpack-dev-server --no-info --hide-modules --hot --debug --devtool eval --output-pathinfo --watch --colors --inline --history-api-fallback --content-base --display-error-details --mode development",
    "server": "nodemon ./server/start.js",
    "watch": "webpack --watch --mode production",
    "ssr": "nodemon ./public/assets/server.bundle.js --ignore node_modules/ --mode development",
    "devSsr": "concurrently \"yarn watch\" \"yarn ssr\"",
    "build": "SET NODE_ENV=production&SET BABEL_ENV=production&webpack -p --mode development",
    "dev": "concurrently \"yarn server\" \"yarn wds\" --names \"server\" --prefix name --mode development",
